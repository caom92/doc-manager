include globals.pug

div.row
  div.col.s12
    h4
      i.mdi.mdi-magnify
      ='{{ langManager.messages.search.title }}'

div.row
  div.col.s12
    mz-collection
      mz-collection-item('*ngFor'='let document of documents')
        +link()('(click)'='onDocumentTypeSelected(document)')
          ='{{ document.name }}'
